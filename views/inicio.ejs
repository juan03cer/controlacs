        <div class="wrapper">
            <i class="bi bi-x" id="close"></i>
    
            <div class="form sign-in">
                <h2>Login</h2>
                <form action="/auth" method="post">
                    <div class="input">
                        <i class="bi bi-person-fill"></i>
                        <input type="text" name="correo" required>
                        <span id="correo">Email</span>
                    </div>
                    <div class="input">
                        <i class="bi bi-lock-fill"></i>
                        <input type="password" name="password" required>
                        <span id="password">Contraseña</span>
                    </div>
                    <div class="condition">
                        <span>
                            <input type="checkbox">Recordar
                        </span>
                        <a href="#">Has olvidado tu contraseña</a>
                    </div>
                    <button type="submit">Iniciar sesión</button>
                    <div class="sign-link">
                        <span>No tienes una cuenta
                            <a href="#" id="sign-up">Registrate</a>
                        </span>
                    </div>
                </form>
                
            </div>
            <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

    <% if (typeof alert != "undefined")  { %>
     <script>
     Swal.fire({
       title:'<%= alertTitle %>',
       text:'<%= alertMessage %>',
       icon:'<%= alertIcon %>',
       showConfirmButton: <%= showConfirmButton %>,
       timer: <%= timer %>
     }).then(()=>{
       window.location='/<%= ruta %>'
     })
     </script>
    <% } %>
    
    <form action="inicio" method="post" class="form sign-up" enctype="multipart/form-data">

                <h2>Registrate</h2>
            
                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="text" name="nombre" required>
                    <span id="nombre">Nombre</span>
                </div>
              
                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="text" name="lastname" required>
                    <span id="lastname">Apellidos</span>
                </div>
                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="email" name="correo" required>
                    <span id="correo">Correo</span>
                </div>

                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="password" name="password" required>
                    <span id="password">Contraseña</span>
                </div>

                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="number" name="claveempleado" required>
                    <span id="claveempleado">Clave De Empleado</span>
                 </div>
                 

                <div class="input">
                    <i class="bi bi-lock-fill"></i>
                    <input type="file" name="foto" required>
                    <span id="foto">Foto</span>
                </div>
                <div>
                <div class="input">
                  <!-- Asegúrate de que el campo de respuesta reCAPTCHA tenga el nombre correcto -->
                  <div class="g-recaptcha" data-sitekey="6LdpvDEUAAAAAMy8x0y8PS99j4BavfO2oBdVTQGZ"></div>
                 </div>
                <br><br>
                <div class="condition">
                    <span>
                        <input type="checkbox" id="terms">Acepto los términos y condiciones
                    </span>
                </div>
            
                <div style="display: inline;" class="sign-link">
                    <span>¿Ya tienes una cuenta?
                        <a href="#" id="sign-in">Iniciar sesión</a>
                    </span>
                </div>
            
                <button type="submit">Registrarse</button>
                    
                </form>
            </div>
       
